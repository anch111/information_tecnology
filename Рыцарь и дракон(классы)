import sys..
import time
import random
from random import randint

class Character:
    def __init__(self, name=None, hp=int, attack=int, crit_attack = int):
        self.crit_attack = crit_attack
        self.attack = attack
        self.hp = hp
        self.name = name
        self.food_out = 0

    def fight(self, other):
        while other.hp > 0 and self.hp > 0:
            print(f'{self.name} –∞—Ç–∞—Ç–∫—É–µ—Ç {other.name}.\n'
                  f'[–ù–∞–∂–º–∏ Enter, —á—Ç–æ–±—ã –±—Ä–æ—Å–∏—Ç—å –∫–æ—Å—Ç–∏...]')
            input('Ô∏èüé≤')
            hod = randint(1, 6)
            print(f'–í—ã–ø–∞–≤—à–µ–µ –∑–Ω–∞—á–µ–Ω–∏–µ: {hod}.')

            if hod > 4:
                print(f'{self.name} –Ω–∞–Ω–æ—Å–∏—Ç –∫—Ä–∏—Ç–∏—á–µ—Å–∫–∏–π —É–¥–∞—Ä.')
                other.hp -= self.crit_attack
            else:
                print(f'{self.name} –Ω–∞–Ω–æ—Å–∏—Ç —É–¥–∞—Ä.')
                other.hp -= self.attack

            if other.hp <= 0:
                print(f'{other.name} –ø–æ—Ä–∞–∂–µ–Ω.')
                break
            else:
                print('–ë–∏—Ç–≤–∞ –ø—Ä–æ–¥–æ–ª–∂–∞–µ—Ç—Å—è...')
                input('üõ°Ô∏èüó°Ô∏è')
            if self.hp <= 0:
                print(f'{self.name} –ø–æ—Ä–∞–∂–µ–Ω.')
                break

            Knight.check_hp(player)


            print(f'{self.name}. –û—Å—Ç–∞–ª–æ—Å—å {self.hp} –µ–¥–∏–Ω–∏—Ü –∑–¥–æ—Ä–æ–≤—å—è.')
            print(f'{other.name}. –û—Å—Ç–∞–ª–æ—Å—å {other.hp} –µ–¥–∏–Ω–∏—Ü –∑–¥–æ—Ä–æ–≤—å—è.')

            Character.fight(other, self)


class Knight(Character):
    def __init__(self, name, hp, attack, crit_attack):
        super().__init__(name, hp, attack, crit_attack)
        self.food_check = 1

    def attack_up(self):
        self.attack += 50

    def crit_attack_up(self):
        self.crit_attack += 50

    def food(self):
        self.hp = 600

    def inventory_check(self):
        if self.food_check == 1:
            queat = input('–°—ä–µ—Å—Ç—å —Å—É—Ö–∞—Ä—å –∏ –≤–æ—Å–ø–æ–ª–Ω–∏—Ç—å –∑–¥–æ—Ä–æ–≤—å–µ?')
            if queat.lower() == '–¥–∞':
                print('–í—ã —Å—ä–µ–ª–∏ —Å—É—Ö–∞—Ä—å. –í–∞—à–µ –∑–¥–æ—Ä–æ–≤—å–µ –≤–æ—Å–ø–æ–ª–Ω–µ–Ω–æ.')
                Knight.food(self)
                self.food_check = 0
        else:
            print("–ö–∞–∂–µ—Ç—Å—è, –∫–∞—Ä–º–∞–Ω –ø—É—Å—Ç.")

    def food_out(self):
        self.food_check = 0

    def check_hp(self):
        if self.hp <= 300 and self.food_check == 1:
            print('–ö–∞–∂–µ—Ç—Å—è, —É –≤–∞—Å –≤ –∫–∞—Ä–º–∞–Ω–µ –±—ã–ª–æ —á—Ç–æ-—Ç–æ, —á—Ç–æ –º–æ–≥–ª–æ –±—ã —Å–µ–π—á–∞—Å –æ—á–µ–Ω—å –ø—Ä–∏–≥–æ–¥–∏—Ç—å—Å—è...')
            Knight.inventory_check(self)

    def low_hp_check(self):
        if self.hp <= 0:
            return True
        return False

    def steal_the_shield(self):
        print_slow('–í–æ—Ä–æ–≤—Å—Ç–≤–æ - —Ç–æ–∂–µ –Ω–∞–≤—ã–∫. –í–∞–º –ø—Ä–µ–¥—Å—Ç–æ–∏—Ç –æ—Ç–≥–∞–¥–∞—Ç—å —á–∏—Å–ª–æ –æ—Ç 1 –¥–æ 3, —á—Ç–æ–±—ã —É–∫—Ä–∞—Å—Ç—å —â–∏—Ç.')
        print_slow('–£ –≤–∞—Å 2 –ø–æ–ø—ã—Ç–∫–∏.')
        random_number = random.randint(1, 3)
        for i in range(2):
            num = int(input('–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ: '))
            print_slow("–í—ã –∫—Ä–∞–¥—ë—Ç–µ—Å—å –∫ –ª–æ—à–∞–¥–∏, —Å—Ç–∞—Ä–∞—è—Å—å –Ω–µ —Å–ø—É–≥–Ω—É—Ç—å –∂–∏–≤–æ—Ç–Ω–æ–µ.")
            print_slow("–í–∞—à–∞ —Ä—É–∫–∞ –ª–æ–∂–∏—Ç—Å—è –Ω–∞ –ø—Ä–æ—Ö–ª–∞–¥–Ω—ã–π –º–µ—Ç–∞–ª–ª —â–∏—Ç–∞... –ï—â—ë –º–≥–Ω–æ–≤–µ–Ω–∏–µ - –∏ –æ–Ω –≤–∞—à!")
            if num == random_number:
                print_slow("‚Äî –≠–π, —á—Ç–æ —ç—Ç–æ —Ç—ã –¥–µ–ª–∞–µ—à—å?! ‚Äî –∫—Ä–∏—á–∏—Ç –ø—É—Ç–Ω–∏–∫, –æ–±–µ—Ä–Ω—É–≤—à–∏—Å—å.")
                print_slow("–ù–æ –≤—ã —É–∂–µ –±–µ–∂–∏—Ç–µ –ø—Ä–æ—á—å —Å –¥–æ—Ä–æ–≥–∏, –ø—Ä–∏–∂–∏–º–∞—è –∫ –≥—Ä—É–¥–∏ –±–ª–µ—Å—Ç—è—â—É—é –¥–æ–±—ã—á—É.")
                print_slow("–ü—É—Ç–Ω–∏–∫ –µ—â—ë –∫–∞–∫–æ–µ-—Ç–æ –≤—Ä–µ–º—è –∫—Ä–∏—á–∏—Ç –≤–∞–º –≤—Å–ª–µ–¥, \n"
                           "–ø—ã—Ç–∞–µ—Ç—Å—è –∑–∞–±—Ä–∞—Ç—å—Å—è –Ω–∞ —É—Å—Ç–∞–≤—à—É—é –ª–æ—à–∞–¥—å –∏ –ø—É—Å—Ç–∏—Ç—å—Å—è –≤ –ø–æ–≥–æ–Ω—é, –Ω–æ —Ç–∞ –Ω–µ –ø–æ–¥–¥–∞–µ—Ç—Å—è.")
                return True
            elif num != random_number and i < 1:
                print_slow("‚Äî –≠–π, —á—Ç–æ —ç—Ç–æ —Ç—ã –¥–µ–ª–∞–µ—à—å?! ‚Äî –∫—Ä–∏—á–∏—Ç –ø—É—Ç–Ω–∏–∫, –æ–±–µ—Ä–Ω—É–≤—à–∏—Å—å.")
                print_slow("–í—ã —É—Å–ø–µ–≤–∞–µ—Ç–µ –æ–¥–µ—Ä–Ω—É—Ç—å —Ä—É–∫—É –∏ –ø—Ä–∏—Ç–≤–æ—Ä–∏—Ç—å—Å—è, —á—Ç–æ —Ä–∞—Å—Å–º–∞—Ç—Ä–∏–≤–∞–µ—Ç–µ —â–∏—Ç.")
                print_slow("‚Äî –ß—Ç–æ, –ø–æ–Ω—Ä–∞–≤–∏–ª—Å—è? ‚Äî —Å —É–ª—ã–±–∫–æ–π –≥–æ—Ä–¥–æ—Å—Ç–∏ —Å–ø—Ä–∞–≤—à–∏–≤–∞–µ—Ç –ø—É—Ç–Ω–∏–∫, –±—É–¥—Ç–æ –Ω–∏—á–µ–≥–æ –∏ –Ω–µ –∑–∞–º–µ—Ç–∏–ª.")
                print_slow("‚Äî –≠—Ç–æ —Ñ–∞–º–∏–ª—å–Ω–∞—è –¥—Ä–∞–≥–æ—Ü–µ–Ω–Ω–æ—Å—Ç—å. –ú–Ω–µ –ø–æ–¥–∞—Ä–∏–ª –µ–≥–æ –æ—Ç–µ—Ü –º–Ω–æ–≥–æ –ª–µ—Ç –Ω–∞–∑–∞–¥. –° —Ç–µ—Ö –ø–æ—Ä –Ω–æ—à—É —Å —Å–æ–±–æ–π - —â–∏—Ç –ø—Ä–∏–Ω–æ—Å–∏—Ç —É–¥–∞—á—É!")
                print_slow("–ü—É—Ç–Ω–∏–∫ —Å–Ω–æ–≤–∞ –æ—Ç–≤–µ—Ä–Ω—É–ª—Å—è.")
                print_slow("–≠—Ç–æ –≤–∞—à —à–∞–Ω—Å –∑–∞–≤–ª–∞–¥–µ—Ç—å —â–∏—Ç–æ–º!")
            elif i == 1:
                print_slow("‚Äî –≠–π, —á—Ç–æ —ç—Ç–æ —Ç—ã –¥–µ–ª–∞–µ—à—å?! ‚Äî –∫—Ä–∏—á–∏—Ç –ø—É—Ç–Ω–∏–∫, –æ–±–µ—Ä–Ω—É–≤—à–∏—Å—å.")
                print_slow("‚Äî –Ø –≤—Å–µ –≤–∏–¥–µ–ª! –£–±–∏—Ä–∞–π –æ—Ç—Ç—É–¥–∞ —Å–≤–æ–∏ —Ä—É—á–æ–Ω–∫–∏ –∏ –ø—Ä–æ–≤–∞–ª–∏–≤–∞–π!")
                print_slow("–ù–∞—Å—É–ø–∏–≤—à–∏—Å—å, –æ–Ω –≤—Å–∫–∞–∫–∏–≤–∞–µ—Ç –Ω–∞ –∫–æ–Ω—è –∏ —É–µ–∑–∂–∞–µ—Ç, –æ–¥–∞—Ä–∏–≤–∞—è –≤–∞—Å –ø—Ä–µ–∑—Ä–∏—Ç–µ–ª—å–Ω—ã–º –≤–∑–≥–ª—è–¥–æ–º.")
                return False

class Dragon(Character):
    def __init__(self, name, hp, attack, crit_attack):
        super().__init__(name, hp, attack, crit_attack)

    def attack_down(self):
        self.attack -= 25

    def crit_attack_down(self):
        self.attack -= 50

class Magician(Character):
    def __init__(self, name, hp, attack, crit_attack):
        super().__init__(name, hp, attack, crit_attack)

    def attack_down(self):
        self.attack -= 20

    def crit_attack_down(self):
        self.attack -= 40

def print_slow(text, delay=0.04):
    for char in text:
        print(char, end='', flush=True)
        time.sleep(delay)
    print()

def prompt():
    input("\n[–ù–∞–∂–º–∏ Enter, —á—Ç–æ–±—ã –ø—Ä–æ–¥–æ–ª–∂–∏—Ç—å...]")


print("–î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å –≤ –º–æ—é –º–∏–Ω–∏ –∏–≥—Ä—É!")
prompt()
hero_name = input('–í–≤–µ–¥–∏—Ç–µ –∏–º—è —Å–≤–æ–µ–≥–æ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞: ')
karma = 0
player = Knight(hero_name, 600, 100, 300)
dragon = Dragon('–î—Ä–∞–∫–æ–Ω', 1000, 150, 450)
magician = Magician('–ú–∞–≥', 700, 125, 400)


print('\n=== –°–æ–Ω –∏–ª–∏ —è–≤—å? ===\n')

print_slow('–°–æ–Ω. –ì—É—Å—Ç–æ–π, –∫–∞–∫ —Å–º–æ–ª–∞. –°–æ–ª–Ω–µ—á–Ω—ã–π —Å–≤–µ—Ç –∑–∞–∫—Ä—ã–≤–∞–µ—Ç –≤—ã—Å–æ–∫–∞—è —Ñ–∏–≥—É—Ä–∞ –≤ —á–µ—Ä–Ω–æ–º –ø–ª–∞—â–µ, –Ω–∞–≤–∏—Å–∞—é—â–∞—è –Ω–∞–¥ –≤–∞–º–∏.\n–í –≤–∞—à–µ–π –≥—Ä—É–¥–∏ –∑–∞–∫–∏–ø–∞—é—Ç —Ç—Ä–µ–≤–æ–≥–∞ –∏ –±–µ—Å–ø–æ–∫–æ–π—Å—Ç–≤–æ. ')
print_slow('–ü—Ä–∏—Å—Ç–∞–ª—å–Ω—ã–π –≤–∑–≥–ª—è–¥ –Ω–µ–∏–∑–≤–µ—Å—Ç–Ω–æ–≥–æ —Å–æ–∑–¥–∞–Ω–∏—è –ø—Ä–æ–¥–æ–ª–∂–∞–µ—Ç –ø—Ä–æ–∂–∏–≥–∞—Ç—å –≤–∞—à–µ —Ç–µ–ª–æ.\n–ö—Ç–æ —ç—Ç–æ? –ü–æ—á–µ–º—É –æ–Ω–∞ –∑–¥–µ—Å—å? –û—Ç–∫—É–¥–∞ –æ–Ω–∞? –ò–∑ —ç—Ç–æ–≥–æ –ª–∏ –º–∏—Ä–∞?')
print_slow('–¢–∏—à–∏–Ω—É –æ–±—Ä—ã–≤–∞–µ—Ç –Ω–∏–∑–∫–∏–π –º–µ—Ç–∞–ª–ª–∏—á–µ—Å–∫–∏–π –≥–æ–ª–æ—Å: \n‚Äî –ü–æ–¥–Ω–∏–º–∞–π—Å—è!')
prompt()

print('\n=== –ü—Ä–æ–±—É–∂–¥–µ–Ω–∏–µ ===\n')

print_slow('–¢–∏—à–∏–Ω–∞. –ü–æ–ª—É–¥–µ–Ω–Ω—ã–π –ª—É—á —Å–æ–ª–Ω—Ü–∞ –Ω–∞–∑–æ–π–ª–∏–≤–æ –ø—Ä–æ–±–∏—Ä–∞–µ—Ç—Å—è –ø–æ–¥ —Ç—è–∂—ë–ª—ã–µ –≤–µ–∫–∏.')
print_slow('–í—ã –æ—Ç–∫—Ä—ã–≤–∞–µ—Ç–µ –≥–ª–∞–∑–∞. –í–æ–∫—Ä—É–≥ –Ω–∏ –¥—É—à–∏: –ª–∏—à—å —á–∏—Å—Ç–æ–µ –ø–æ–ª–µ, –æ–∫—É—Ç–∞–Ω–Ω–æ–µ –¥—ã–º–∫–æ–π, –∏–∑–≤–∏–≤–∞—é—â–∞—è—Å—è –¥–æ—Ä–æ–≥–∞ –∏ –æ–¥–∏–Ω–æ–∫–æ–µ –¥–µ—Ä–µ–≤–æ,\n—á—å–∏ –≤–µ—Ç–≤–∏ —É–∫—Ä—ã–≤–∞—é—Ç –≤–∞—Å –æ—Ç –ø–∞–ª—è—â–µ–≥–æ —Å–æ–ª–Ω—Ü–∞.')
print_slow('–ì–æ–ª–æ–≤–∞ —Ä–∞—Å–∫–∞–ª—ã–≤–∞–µ—Ç—Å—è. –ü–∞–º—è—Ç—å - –∑—ã–±–∫–∏–π –¥—ã–º. –ü–æ—Å–ª–µ–¥–Ω–µ–µ, —á—Ç–æ –≤—ã –ø–æ–º–Ω–∏—Ç–µ - —Ä–∞–∑–º—ã—Ç—ã–π –æ–±—Ä–∞–∑ —Ñ–∏–≥—É—Ä—ã –≤ —á–µ—Ä–Ω–æ–º –ø–ª–∞—â–µ.\n–ö–∞–∂–µ—Ç—Å—è, —ç—Ç–æ –±—ã–ª –≤—Å–µ–≥–æ –ª–∏—à—å —Å–æ–Ω.')
print_slow('–í—ã –æ—Å–º–∞—Ç—Ä–∏–≤–∞–µ—Ç–µ—Å—å. –í–∞—à–∏ –¥–æ—Å–ø–µ—Ö–∏ —Å–ª–æ–º–∞–Ω—ã, –ø–æ–∫—Ä—ã—Ç—ã –±–∞–≥—Ä–æ–≤—ã–º–∏ –∫–∞–ø–ª—è–º–∏ –∑–∞–ø–µ–∫—à–µ–π—Å—è –∫—Ä–æ–≤–∏.\n–ß—Ç–æ –∂–µ –ø—Ä–æ–∏–∑–æ—à–ª–æ? –°–∫–æ–ª—å–∫–æ –ø—Ä–æ—à–ª–æ –≤—Ä–µ–º–µ–Ω–∏? ')
print_slow('–ù–∞ –æ—â—É–ø—å –≤ –∫–∞—Ä–º–∞–Ω–µ –≤—ã –Ω–∞—Ö–æ–¥–∏—Ç–µ –µ–¥–∏–Ω—Å—Ç–≤–µ–Ω–Ω—É—é –ø—Ä–∏–≥–æ—Ä—à–Ω—é —Å—É—Ö–∞—Ä—è. –ï–¥–∞.' )
prompt()

print('\n=== –î–æ—Ä–æ–≥–∞ –≤ –Ω–∏–∫—É–¥–∞ ===\n')

print_slow('–í—ã –ø–æ–¥–Ω–∏–º–∞–µ—Ç–µ—Å—å –∏ –≤—ã—Ö–æ–¥–∏—Ç–µ –Ω–∞ –¥–æ—Ä–æ–≥—É. –ù–µ–¥–∞–ª–µ–∫–æ –≤–∏–¥–µ–Ω —É–∫–∞–∑–∞—Ç–µ–ª—å —Å –∫—Ä–∏–≤–æ–π –Ω–∞–¥–ø–∏—Å—å—é "–ù–∏–ª—å—Ñ–≥–∞–∞—Ä–¥".')
print_slow('–ù–∞–∑–≤–∞–Ω–∏–µ –≥–æ—Ä–æ–¥–∞ –∫–∞–∂–µ—Ç—Å—è –∑–Ω–∞–∫–æ–º—ã–º. –í—ã —Ä–µ—à–∞–µ—Ç–µ –Ω–∞–ø—Ä–∞–≤–∏—Ç—å—Å—è –≤ —É–∫–∞–∑–∞–Ω–Ω–æ–º –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–∏.')
print_slow('–°–ø—É—Å—Ç—è –Ω–µ—Å–∫–æ–ª—å–∫–æ —á–∞—Å–æ–≤ –∏–∑–Ω—É—Ä—è—é—â–µ–π —Ö–æ–¥—å–±—ã, –≤—ã —Å–ø–æ—Ç—ã–∫–∞–µ—Ç–µ—Å—å –æ –º–µ—Ç–∞–ª–ª–∏—á–µ—Å–∫–∏–π –ø—Ä–µ–¥–º–µ—Ç.')
print_slow('‚Äî –ú–æ–π –º–µ—á!')
print_slow('–ù–∞ –∑–µ–º–ª–µ –ª–µ–∂–∏—Ç –≤–∞—à –º–µ—á: —ç—Ñ–µ—Å –Ω–∞–¥–ª–æ–º–∞–Ω, –ª–µ–∑–≤–∏–µ –∑–∞–∑—É–±—Ä–µ–Ω–æ. –ù–µ—Å–º–æ—Ç—Ä—è –Ω–∞ –ø–ª–∞—á–µ–≤–Ω–æ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ, –º–µ—á –≤—Å–µ –µ—â—ë –º–æ–∂–µ—Ç –ø—Ä–∏–≥–æ–¥–∏—Ç—å—Å—è –≤ –±–æ—é.')
print_slow('–í—ã –ø–æ–¥–±–∏—Ä–∞–µ—Ç–µ –º–µ—á.')
print_slow('–í—ã –ø—Ä–æ–¥–æ–ª–∂–∞–µ—Ç–µ —Å–≤–æ–π –Ω–µ–ª–µ–≥–∫–∏–π –ø—É—Ç—å, –ø—Ä–æ—Ö–æ–¥—è —Å–∫–≤–æ–∑—å –ø–æ–ª—è –∏ –ª—É–≥–∞, –º–∏–Ω—É—è –∑–∞–º—à–µ–ª—ã–µ –≤–∞–ª—É–Ω—ã –≤–¥–æ–ª—å –∫–∞–º–µ–Ω–∏—Å—Ç–æ–π –¥–æ—Ä–æ–≥–∏.')
print_slow('–í–¥—Ä—É–≥, —Å—Ä–µ–¥–∏ —á–∏—Å—Ç–æ–≥–æ –ø–æ–ª—è, –≥–¥–µ, –∫–∞–∑–∞–ª–æ—Å—å, –Ω–µ—Ç –Ω–∏ –æ–¥–Ω–æ–π –∂–∏–≤–æ–π –¥—É—à–∏, –≤—ã –≤–∏–¥–∏—Ç–µ –æ–¥–∏–Ω–æ–∫–æ–≥–æ –ø—É—Ç–Ω–∏–∫–∞ –≤–µ—Ä—Ö–æ–º –Ω–∞ –∑–∞–≥–Ω–∞–Ω–Ω–æ–π —É—Å—Ç–∞–≤—à–µ–π –∫–ª—è—á–µ.')
print_slow('–û–Ω —Å–∫–∞—á–µ—Ç –ø—Ä—è–º–æ –≤–∞–º –Ω–∞–≤—Å—Ç—Ä–µ—á—É. –°—Ä–µ–¥–∏ —Ç—é–∫–æ–≤ –∏ –º–µ—à–∫–æ–≤, —á—Ç–æ –≤–∑–≤–∞–ª–µ–Ω—ã –Ω–∞ –ª–æ—à–∞–¥—å, –≤—ã –∑–∞–º–µ—á–∞–µ—Ç–µ –±–ª–µ—Å—Ç—è—â–∏–π —â–∏—Ç —Å –Ω–µ–æ–±—ã—á–Ω—ã–º –≥–µ—Ä–±–æ–º.')
print_slow('‚Äî –ö—É–¥–∞ –ø—É—Ç—å –¥–µ—Ä–∂–∏—à—å, –±—Ä–∞–≤—ã–π –≤–æ–∏–Ω? ‚Äî –≤–µ—Å–µ–ª–æ –æ–∫–ª–∏–∫–∞–µ—Ç –≤–∞—Å –ø—É—Ç–Ω–∏–∫.\n'
           '‚Äî –í –≥–æ—Ä–æ–¥, —á—Ç–æ –∑–æ–≤—É—Ç –ù–∏–ª—å—Ñ–≥–∞–∞—Ä–¥–æ–º. –°–∫–∞–∂–∏, –¥–∞–ª–µ–∫–æ –ª–∏ –µ—â–µ –¥–æ —Ç—É–¥–∞?')
print_slow("‚Äî –û, –ù–∏–ª—å—Ñ–≥–∞–∞—Ä–¥! ‚Äî –ø—É—Ç–Ω–∏–∫ —à–∏—Ä–æ–∫–æ —É–ª—ã–±–∞–µ—Ç—Å—è. ‚Äî –î–∞ —Ç—É—Ç —Ä—É–∫–æ–π –ø–æ–¥–∞—Ç—å! –í—Å–µ–≥–æ –æ–∫–æ–ª–æ –º–∏–ª–∏!")
print_slow("–û–Ω —Å–ª–µ–∑–∞–µ—Ç —Å –ª–æ—à–∞–¥–∏, —á—Ç–æ–±—ã –ø–æ–ø—Ä–∞–≤–∏—Ç—å –ø–æ–∫–ª–∞–∂—É. –©–∏—Ç —Ç–µ–ø–µ—Ä—å –≤–∏—Å–∏—Ç —Å–æ–≤—Å–µ–º –±–µ–∑ –ø—Ä–∏—Å–º–æ—Ç—Ä–∞...")

print('–ü–µ—Ä–µ–¥ –≤–∞–º–∏ —Å—Ç–æ–∏—Ç –≤—ã–±–æ—Ä:\n'
           '1. –£–∫—Ä–∞—Å—Ç—å —â–∏—Ç, –ø–æ–∫–∞ –ø—É—Ç–Ω–∏–∫ –æ—Ç–≤–ª–µ–∫—Å—è.\n'
           '2. –ü–æ–±–ª–∞–≥–æ–¥–∞—Ä–∏—Ç—å –∑–∞ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –∏ –ø—Ä–æ–¥–æ–ª–∂–∏—Ç—å –ø—É—Ç—å.')

choice = input('–ß—Ç–æ –≤—ã –≤—ã–±–µ—Ä–∏—Ç–µ?: ')

if choice == "1":
    if Knight.steal_the_shield(player):
        print_slow("–°—Ä–µ–¥–∏ –¥–µ—Ä–µ–≤—å–µ–≤ –≤—ã –≤–∏–¥–∏—Ç–µ –∑–Ω–∞–∫–æ–º—ã–π —Å–∏–ª—É—ç—Ç –≤ —á–µ—Ä–Ω–æ–º –ø–ª–∞—â–µ.")
        print_slow("–ö–∞–∂–µ—Ç—Å—è, –≤–∞–º –∏–∑—Ä—è–¥–Ω–æ –Ω–∞–ø–µ–∫–ª–æ –≥–æ–ª–æ–≤—É.")
        karma += 1
        Dragon.attack_down(dragon)
        Dragon.crit_attack_down(dragon)
        Magician.attack_down(magician)
        Magician.crit_attack_down(magician)
        prompt()
    else:
        prompt()
else:
    print_slow("\n‚Äî –ë–ª–∞–≥–æ–¥–∞—Ä—é –∑–∞ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é, ‚Äî –≤–µ–∂–ª–∏–≤–æ –≥–æ–≤–æ—Ä–∏—Ç–µ –≤—ã.")
    print_slow(
        "‚Äî –ù–µ –∑–∞ —á—Ç–æ, —Ä—ã—Ü–∞—Ä—å! ‚Äî —Ç–æ—Ç —Å–Ω–æ–≤–∞ —É—Å–∞–∂–∏–≤–∞–µ—Ç—Å—è –≤ —Å–µ–¥–ª–æ. ‚Äî –£–¥–∞—á–∏ –≤ –ù–∏–ª—å—Ñ–≥–∞–∞—Ä–¥–µ!")
    print_slow("–ü—É—Ç–Ω–∏–∫ –º–∞—à–µ—Ç —Ä—É–∫–æ–π –∏ –ø—Ä–æ–¥–æ–ª–∂–∞–µ—Ç —Å–≤–æ–π –ø—É—Ç—å.")
    print_slow("–©–∏—Ç —Ç–∞–∫ –∏ –æ—Å—Ç–∞—ë—Ç—Å—è –≤–∏—Å–µ—Ç—å –Ω–∞ –µ–≥–æ –ª–æ—à–∞–¥–∏, —Å–≤–µ—Ä–∫–∞—è –Ω–∞ —Å–æ–ª–Ω—Ü–µ.")
    print_slow("\n–í—ã —á—É–≤—Å—Ç–≤—É–µ—Ç–µ, —á—Ç–æ –ø–æ—Å—Ç—É–ø–∏–ª–∏ –ø—Ä–∞–≤–∏–ª—å–Ω–æ, –∫–∞–∫ –ø–æ–¥–æ–±–∞–µ—Ç –Ω–∞—Å—Ç–æ—è—â–µ–º—É —Ä—ã—Ü–∞—Ä—é.")
    prompt()

print('\n=== –ü–µ–ø–µ–ª–∏—â–µ ===\n')
print_slow('–°–ø—É—Å—Ç—è –Ω–µ–æ–ø—Ä–µ–¥–µ–ª–µ–Ω–Ω–æ–µ –≤—Ä–µ–º—è, –≤—ã –≤–∏–¥–∏—Ç–µ –Ω–∞ –≥–æ—Ä–∏–∑–æ–Ω—Ç–µ –≥–æ—Ä–æ–¥, —Ä–∞—Å–∫–∏–Ω—É–≤—à–∏–π—Å—è —É –ø–æ–¥–Ω–æ–∂–∏—è –≥–æ—Ä.')
print_slow('–ü–æ–¥–æ–π–¥—è –±–ª–∏–∂–µ, –≤—ã —Å–º–æ—Ç—Ä–∏—Ç–µ –Ω–∞ —Ç–æ, —á—Ç–æ –æ—Å—Ç–∞–ª–æ—Å—å –æ—Ç –Ω–µ–∫–æ–≥–¥–∞ –≤–µ–ª–∏—á–µ—Å—Ç–≤–µ–Ω–Ω–æ–≥–æ –ù–∏–ª—å—Ñ–≥–∞–∞—Ä–¥–∞: –æ–±—É–≥–ª–µ–Ω–Ω—ã–µ —Å–∫–µ–ª–µ—Ç—ã –¥–æ–º–æ–≤.'
           ' –¢–∏—à–∏–Ω–∞, –ø–∞—Ö–Ω—É—â–∞—è –≥–∞—Ä—å—é –∏ —Å–º–µ—Ä—Ç—å—é. \n–ß—Ç–æ –∑–¥–µ—Å—å –ø—Ä–æ–∏–∑–æ—à–ª–æ?')
print_slow('–ü–æ–±—Ä–æ–¥–∏–≤ –ø–æ –æ—Å—Ç–∞–Ω–∫–∞–º –≥–æ—Ä–æ–¥–∞ –≤—ã –Ω–∞—Ç–∫–∞–µ—Ç–µ—Å—å –Ω–∞ —Ä–∞–∑—Ä—É—à–µ–Ω–Ω—É—é –∫—É–∑–Ω–∏—Ü—É. –¢–∞–º –º–æ–∂–Ω–æ –±—ã–ª–æ –±—ã —Ä–∞–∑–¥–æ–±—ã—Ç—å –Ω–æ–≤—ã–µ –¥–æ—Å–ø–µ—Ö–∏ –∏ –º–µ—á. –í—ã –∑–∞—Ö–æ–¥–∏—Ç–µ.')
print_slow("–í–Ω—É—Ç—Ä–∏ —Ü–∞—Ä–∏—Ç —Ö–∞–æ—Å. –°—Ä–µ–¥–∏ —Ä–∞–∑–±—Ä–æ—Å–∞–Ω–Ω—ã—Ö –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç–æ–≤ –¥–≤–∞ —Ç–µ–ª–∞.")
print_slow("–ö—É–∑–Ω–µ—Ü. –ú—ë—Ä—Ç–≤.")
print_slow("–†—è–¥–æ–º —Ä—ã—Ü–∞—Ä—å –≤ —Å–∏—è—é—â–∏—Ö –¥–æ—Å–ø–µ—Ö–∞—Ö.")

print_slow('–û–Ω —Å—Ç–∞—Ä–∞–ª—Å—è –∑–∞—â–∏—Ç–∏—Ç—å –∫—É–∑–Ω–µ—Ü–∞, –Ω–æ –Ω–µ —Å—É–º–µ–ª –∏ –ø–æ–≥–∏–± —Ö—Ä–∞–±—Ä–æ–π —Å–º–µ—Ä—Ç—å—é.\n'
           '–ù–æ –æ—Ç —á–µ–≥–æ? –û—Ç —á–µ–≥–æ –ø–æ–≥–∏–±–ª–∏ —ç—Ç–∏ –Ω–µ–≤–∏–Ω–Ω—ã–µ –¥—É—à–∏?')
print_slow('–í—ã –ø—Ä–∏—Å—Ç–∞–ª—å–Ω–µ–π –ø—Ä–∏—Å–º–∞—Ç—Ä–∏–≤–∞–µ—Ç–µ—Å—å –∫ –Ω–æ–≤—ã–º —Ä—ã—Ü–∞—Ä—Å–∫–∏–º –¥–æ—Å–ø–µ—Ö–∞–º.')
print('–ü–µ—Ä–µ–¥ –≤–∞–º–∏ —Å—Ç–æ–∏—Ç –≤—ã–±–æ—Ä:\n'
        '1. –ó–∞–±—Ä–∞—Ç—å –¥–æ—Å–ø–µ—Ö–∏, –æ—Å–∫–≤–µ—Ä–Ω–∏–≤ —Ç–µ–ª–æ –ø–∞–≤—à–µ–≥–æ.\n'
        '2. –ü—Ä–æ—è–≤–∏—Ç—å —É–≤–∞–∂–µ–Ω–∏–µ. –ù–µ –±—Ä–∞—Ç—å –¥–æ—Å–ø–µ—Ö–∏.\n')

choice1 = input('–ß—Ç–æ –≤—ã –≤—ã–±–µ—Ä–∏—Ç–µ?: ')
if choice1 == '1':
    Dragon.attack_down(dragon)
    Dragon.crit_attack_down(dragon)
    Magician.attack_down(magician)
    Magician.crit_attack_down(magician)
    karma += 1
    print_slow('–í—ã —Å–¥–∏—Ä–∞–µ—Ç–µ –¥–æ—Å–ø–µ—Ö–∏ —Å –º–µ—Ä—Ç–≤–æ–≥–æ —Ç–µ–ª–∞. –ü–æ–≥–∏–±—à–µ–º—É —Ä—ã—Ü–∞—Ä—é –æ–Ω–∏ –≤—Å–µ —Ä–∞–≤–Ω–æ —É–∂–µ –Ω–µ –ø–æ–Ω–∞–¥–æ–±—è—Ç—Å—è.')
    print_slow('–í—ã —Ä–∞–∑–≤–æ—Ä–∞—á–∏–≤–∞–µ—Ç–µ—Å—å, —Å–æ–±—Ä–∞–≤—à–∏—Å—å –≤—ã–π—Ç–∏ –∏–∑ –∫—É–∑–Ω–∏—Ü—ã, –∫–∞–∫ –≤–¥—Ä—É–≥ –≥—Ä—É–¥—å –ø—Ä–æ–∂–∏–≥–∞–µ—Ç —á—É–≤—Ç—Å–≤–æ —Ç—Ä–µ–≤–æ–≥–∏.')
    print_slow('–í—ã –≤–∏–¥–∏—Ç–µ —Ñ–∏–≥—É—Ä—É, –∑–∞–∫—É—Ç–∞–Ω–Ω—É—é –≤ —á–µ—Ä–Ω—ã–π –ø–ª–∞—â.')
    print_slow('–û–Ω–∞ —Å—Ç–æ–∏—Ç –Ω–µ–¥–∞–ª–µ–∫–æ –æ—Ç –≤–∞—Å. –í—ã –Ω–µ –≤–∏–¥–∏—Ç–µ –Ω–∏ –≥–ª–∞–∑, –Ω–∏ –ª–∏—Ü–∞, –Ω–æ —á—É—Ç–≤—Å—Ç–≤—É–µ—Ç–µ –Ω–∞ —Å–µ–±–µ –ø—Ä–∏—Å—Ç–∞–ª—å–Ω—ã–π –≤–∑–≥–ª—è–¥.')
    print_slow('–í—ã —Å–º–æ—Ç—Ä–∏—Ç–µ –Ω–∞ —Ç–µ–ª–∞ –∫—É–∑–Ω–µ—Ü–∞ –∏ —Ä—ã—Ü–∞—Ä—è, –∑–∞—Ç–µ–º - –≤–Ω–æ–≤—å –ø–µ—Ä–µ–≤–æ–¥–∏—Ç–µ –≤–∑–≥–ª—è–¥ –Ω–∞ —Ñ–∏–≥—É—Ä—É.')
    print_slow('–¢–∞–º, –≥–¥–µ, —Ç–æ–ª—å–∫–æ —á—Ç–æ, –∫–∞–∑–∞–ª–æ—Å—å, –ª–µ–∂–∞–ª–∏ –ø–æ–¥–æ–ª—ã —á–µ—Ä–Ω–æ–≥–æ –ø–ª–∞—â–∞, –≤–Ω–æ–≤—å –≤–∏–¥–µ–Ω –≥–æ–ª—ã–π –ø–æ–ª –∫—É–∑–Ω–∏—Ü—ã.\n'
               '–û–Ω–∞ –±—É–¥—Ç–æ –∏—Å–ø–∞—Ä–∏–ª–∞—Å—å. –ö–∞–∫ –∏—Å—á–µ–∑–ª–æ –∏ –¥–∞–≤—è—â–µ–µ —á—É—Å—Ç–≤–æ –≤ –≥—Ä—É–¥–∏.\n'
               '–ü–æ–∫–∞–∑–∞–ª–æ—Å—å.')
else:
    print_slow('–í—ã –æ—Å—Ç–∞–≤–ª—è–µ—Ç–µ –¥–æ—Å–ø–µ—Ö–∏ –Ω–µ—Ç—Ä–æ–Ω—É—Ç—ã–º–∏. –ü—É—Å—Ç—å –æ–Ω —É–ø–æ–∫–æ–∏—Ç—Å—è —Å –º–∏—Ä–æ–º, –∏—Å–ø–æ–ª–Ω–∏–≤ —Å–≤–æ–π –¥–æ–ª–≥.')

print_slow('–í—ã –≤—ã—Ö–æ–¥–∏—Ç–µ –∏–∑ –∫—É–∑–Ω–∏—Ü—ã, –ø—Ä–æ–¥–æ–ª–∂–∞—è –±–µ—Å—Ü–µ–ª—å–Ω–æ–µ —Å–∫–∏—Ç–∞–Ω–∏–µ –ø–æ –ø—É—Å—Ç—ã–º —É–ª–æ—á–∫–∞–º –ù–∏–ª—å—Ñ–≥–∞–∞—Ä–¥–∞.')
print_slow('–í –æ–¥–Ω–æ–º –∏–∑ –ø–æ–ª—É—Ä–∞–∑—Ä—É—à–µ–Ω–Ω—ã—Ö –¥–æ–º–æ–≤ –º—ã —Å–ª—ã—à–∏—Ç–µ —à–æ—Ä–æ—Ö. –ü–æ—Ö–æ–∂–µ –Ω–∞ –≥–æ—Ä–æ—Ö, —Ä–∞—Å—Å—ã–ø–∞—é—â–∏–π—Å—è –ø–æ –ø–æ–ª—É.')
print('–ü–µ—Ä–µ–¥ –≤–∞–º–∏ —Å—Ç–æ–∏—Ç –≤—ã–±–æ—Ä:\n'
           '1. –ü—Ä–æ–≤–µ—Ä–∏—Ç—å, —á—Ç–æ —Å–∫—Ä—ã–≤–∞–µ—Ç—Å—è –≤ —Ç–µ–º–Ω–æ—Ç–µ —Ä–∞–∑—Ä—É—à–µ–Ω–Ω–æ–≥–æ –¥–æ–º–∞.\n'
           '2. –û–±–æ–π—Ç–∏ –ø–æ–¥–æ–∑—Ä–∏—Ç–µ–ª—å–Ω—ã–π –¥–æ–º —Å—Ç–æ—Ä–æ–Ω–æ–π.')

choice2 = input('–ß—Ç–æ –≤—ã –≤—ã–±–µ—Ä–∏—Ç–µ?: ')
if choice2 == '1':
    print_slow('–í—ã –∑–∞—Ö–æ–¥–∏—Ç–µ –≤ –¥–æ–º –∏ –≤–∏–¥–∏—Ç–µ —É–ø–∏—Ç–∞–Ω–Ω—É—é –∫—Ä—ã—Å—É. –ó–∞–º–µ—Ç–∏–≤ –≤–∞—Å, –∂–∏–≤–æ—Ç–Ω–æ–µ —Å–∫—Ä—ã–≤–∞–µ—Ç—Å—è –≤ —â–µ–ª—è—Ö —Å—Ç–µ–Ω.')
    print_slow('–í–∞—à –≤–∑–≥–ª—è–¥ –ø—Ä–∏–≤–ª–µ–∫–∞–µ—Ç –±–ª–µ—Å—Ç—è—â–∏–π –ø—Ä–µ–¥–º–µ—Ç. \n'
               '–ú–µ—á!')
    print_slow('–ö–∞–∂–µ—Ç—Å—è –≤ –¥–æ–º–µ –∫–æ–≥–¥–∞-—Ç–æ –∂–∏–ª –±—Ä–∞–≤—ã–π –≤–æ–∏–Ω. –û–Ω –æ—Å—Ç–∞–≤–∏–ª —Å–≤–æ–π –º–µ—á –¥–æ–±—Ä–æ–≤–æ–ª—å–Ω–æ.')
    print_slow('–í—ã –ø–æ–¥–±–∏—Ä–∞–µ—Ç–µ –Ω–æ–≤—ã–π –º–µ—á –∏ –≤—ã—Ö–æ–¥–∏—Ç–µ –Ω–∞ —É–ª–∏—Ü—É.')
    Knight.attack_up(player)
    Knight.crit_attack_up(player)
else:
    print_slow('–í—ã –ø—Ä–æ—Ö–æ–¥–∏—Ç–µ –º–∏–º–æ –¥–æ–º–∞, –≥–¥–µ —Å—Ç–∏—Ö–∞–µ—Ç –ø–æ–¥–æ–∑—Ä–∏—Ç–µ–ª—å–Ω—ã–π —à–æ—Ä–æ—Ö.')

print_slow('\n–°–æ–ª–Ω—Ü–µ –∑–∞–∫–∞—Ç–∏–ª–æ—Å—å –∑–∞ –≥–æ—Ä–∏–∑–æ–Ω—Ç. –ù–∞–¥ –≥–æ—Ä–æ–¥–æ–º –Ω–∞–≤–∏—Å–ª–∏ —Å—É–º–µ—Ä–∫–∏.\n'
           '–ë—É–¥—Ç–æ —Ñ–∏–∑–∏—á–µ—Å–∫–∏ –æ—â—É—â–∞–µ—Ç—Å—è –¥–∞–≤–ª–µ–Ω–∏–µ —Ç–µ–º–Ω–æ—Ç—ã –∏ –Ω–∞–ø—Ä—è–∂–µ–Ω–Ω–æ–µ –º–æ–ª—á–∞–Ω–∏–µ –ù–∏–ª—å—Ñ–≥–∞–∞—Ä–¥–∞.')
print_slow('–ü—É—Å—Ç—ã–µ —É–ª–∏—Ü—ã –≥–æ—Ä–æ–¥–∞ –ø—Ä–∏–≤–æ–¥—è—Ç –≤–∞—Å –∫ —Å—Ç–∞—Ä–æ–π –±–∞—à–Ω–µ, –≤–æ–∑–≤—ã—à–∞—é—â–µ–π—Å—è –Ω–∞ –æ–∫—Ä–∞–∏–Ω–µ.\n'
           '–°—Ç—Ä–∞–Ω–Ω–æ. –ï–µ –ø–æ—á—Ç–∏ –Ω–µ –∑–∞–¥–µ–ª–∞ –∫–∞—Ç–∞—Å—Ç—Ä–æ—Ñ–∞, –Ω–∞—Å—Ç–∏–≥–Ω—É–≤—à–∞—è –≥–æ—Ä–æ–¥.')
prompt()

print('\n=== –°—Ç–∞—Ä–∞—è –±–∞—à–Ω—è ===\n')

print_slow('–í—ã –∑–∞—Ö–æ–¥–∏—Ç–µ –≤ –±–∞—à–Ω—é. –°—É–¥—è –ø–æ —Ä–∞–∑–Ω–æ—Ü–≤–µ—Ç–Ω—ã–º —Å–∫–ª—è–Ω–∫–∞–º –∏ –¥—Ä–µ–≤–Ω–∏–º –∫–Ω–∏–≥–∞–º, –∑–¥–µ—Å—å –∫–æ–≥–¥–∞-—Ç–æ –∂–∏–ª –º–∞–≥.')
print_slow('–ü—Ä–æ–π–¥—è —á—É—Ç—å –¥–∞–ª—å—à–µ –≤—ã —Å–ª—ã—à–∏—Ç–µ —Å–ª–∞–±—ã–π —Å—Ç–æ–Ω. –í —É–≥–ª—É –¥–∞–ª—å–Ω–µ–π –∫–æ–º–Ω–∞—Ç—ã, —É –ø–æ—Ç—É—Ö—à–µ–≥–æ –∫–∞–º–∏–Ω–∞, —Å—ä–µ–∂–∏–≤—à–∏—Å—å, —Å–∏–¥–∏—Ç —Å—Ç–∞—Ä–∏–∫.\n'
           '–û–Ω –Ω–µ –ø–æ—Ö–æ–∂ –Ω–∞ –º–∞–≥–∞.')
print_slow("‚Äî –ï–¥—ã... ‚Äî —Ö—Ä–∏–ø–∏—Ç –æ–Ω. ‚Äî –î–∞–π—Ç–µ —Å—Ç–∞—Ä–∏–∫—É –µ–¥—ã...")
print_slow("–í—ã –≤—Å–ø–æ–º–∏–Ω–∞–µ—Ç–µ –ø—Ä–æ —Å—É—Ö–∞—Ä—å, –Ω–∞–π–¥–µ–Ω–Ω—ã–π –≤ –∫–∞—Ä–º–∞–Ω–µ.")
print("–ü–µ—Ä–µ–¥ –≤–∞–º–∏ —Å—Ç–æ–∏—Ç –≤—ã–±–æ—Ä:\n"
        "1. –û—Ç–¥–∞—Ç—å –µ–¥—É.\n"
        "2. –ù–µ –æ—Ç–¥–∞–≤–∞—Ç—å. –°–∞–º–æ–º—É —Å–≥–æ–¥–∏—Ç—Å—è.")

choice3 = input('–ß—Ç–æ –≤—ã –≤—ã–±–µ—Ä–∏—Ç–µ?: ')
if choice3 == '1':
    print_slow('–í—ã –æ—Ç–¥–∞–µ—Ç–µ —Å—Ç–∞—Ä–∏–∫—É –∑–∞—á–µ—Ä—Å—Ç–≤–µ–≤—à–∏–π —Å—É—Ö–∞—Ä—å. –í –µ–≥–æ –≥–ª–∞–∑–∞—Ö –≤—Å–ø—ã—Ö–∏–≤–∞–µ—Ç –±–ª–∞–≥–æ–¥–∞—Ä–Ω–∞—Å—Ç—å.')
    print_slow('–ú–æ–∂–µ—Ç –æ–Ω –∑–Ω–∞–µ—Ç, —á—Ç–æ –ø—Ä–æ–∏–∑–æ—à–ª–æ –≤ –≥–æ—Ä–æ–¥–µ? –°—Ç–æ–∏—Ç —Å–ø—Ä–æ—Å–∏—Ç—å.')
    print_slow('–ö–∞–∫ —Ç–æ–ª—å–∫–æ –≤—ã —Å–æ–±–∏—Ä–∞–µ—Ç–µ—Å—å –∑–∞–¥–∞—Ç—å —Å—Ç–∞—Ä–∏–∫—É –≤–æ–ø—Ä–æ—Å, –æ–Ω —Å—Ä—ã–≤–∞–µ—Ç—Å—è —Å –º–µ—Å—Ç–∞, –≤—ã–±–µ–≥–∞–µ—Ç –∏–∑ –±–∞—à–Ω–∏ –∏ —Å–∫—Ä—ã–≤–∞–µ—Ç—Å—è –ø–æ–¥ –ø–æ–∫—Ä–æ–≤–æ–º –Ω–æ—á–∏, \n'
               '–í–æ—Ç —Ç–µ–±–µ –∏ –±–ª–∞–≥–æ–¥–∞—Ä–Ω–æ—Å—Ç—å...')
    Knight.food_out(player)
else:
    karma += 1
    print_slow('–í—ã –æ—Ç–∫–∞–∑—ã–≤–∞–µ—Ç–µ —Å—Ç–∞—Ä–∏–∫—É.')
    print_slow("‚Äî –ñ–∞–ª—å... ‚Äî —à–µ–ø—á–µ—Ç —Å—Ç–∞—Ä–∏–∫, –∏ –µ–≥–æ –≤–∑–≥–ª—è–¥ –ø–æ—Ç—É—Ö–∞–µ—Ç. –í—ã –ø—Ä—è—á–µ—Ç–µ —Å—É—Ö–∞—Ä—å –æ–±—Ä–∞—Ç–Ω–æ.")
    print_slow('–û—Ç–≤–µ—Ä–Ω—É–≤—à–∏—Å—å –æ—Ç —Å—Ç–∞—Ä–∏–∫–∞, –≤—ã —Å–Ω–æ–≤–∞ –≤–∏–¥–∏—Ç–µ –µ–µ...')
    print_slow('–¢—å–º–∞, –ª—å—é—â–∞—è—Å—è –∏–∑-–ø–æ–¥ –∫–∞–ø—é—à–æ–Ω–∞ —á–µ—Ä–Ω–æ–≥–æ –ø–ª–∞—â–∞ –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∞ –ø—Ä—è–º–æ –Ω–∞ –≤–∞—Å. –ß—É–≤—Å—Ç–≤–æ —É–∂–∞—Å–∞ –ø—Ä–æ–Ω–∏–∑—ã–≤–∞–µ—Ç –≤—Å–µ —Ç–µ–ª–æ.')
    print_slow('–ö—Ç–æ —ç—Ç–æ? –ü–æ—á–µ–º—É –º–æ–ª—á–∏—Ç –∏ –ª–∏—à—å —Å–º–æ—Ç—Ä–∏—Ç?')
    print_slow('–î–æ–ª–≥–æ –µ—â–µ –±—É–¥–µ—Ç –ø—Ä–æ–¥–æ–ª–∂–∞—Ç—å—Å—è —ç—Ç–∞ –ø—ã—Ç–∫–∞?')
    print_slow('–ù—É–∂–Ω–æ –ø–æ–ª–æ–∂–∏—Ç—å —ç—Ç–æ–º—É –∫–æ–Ω–µ—Ü!')
    print_slow('–í—ã —É—Å—Ç—Ä–µ–º–ª—è–µ—Ç–µ—Å—å –∫ —Ñ–∏–≥—É—Ä–µ –≤ —á–µ—Ä–Ω–æ–º –ø–ª–∞—â–µ.')
    print_slow('–í–∞—à–∞ —Ä—É–∫–∞ –ø–æ—á—Ç–∏ –∫–∞—Å–∞–µ—Ç—Å—è —Ç–µ–º–Ω–æ–π —Ç–∫–∞–Ω–∏, –∫–∞–∫ –≤–¥—Ä—É–≥ —Ñ–∏–≥—É—Ä–∞ –±–µ—Å—Å–ª–µ–¥–Ω–æ –∏—Å—á–µ–∑–∞–µ—Ç. –†–∞—Å—Ç–≤–æ—Ä—è–µ—Ç—Å—è –≤ –≤–æ–∑–¥—É—Ö–µ.')
    print_slow('–ù—É, –≤ –¥—Ä—É–≥–æ–π —Ä–∞–∑ –ø–æ–≥–æ–≤–æ—Ä–∏–º.')

print_slow('–û–±—ã—Å–∫–∞–≤ –±–∞—à–Ω—é, –≤—ã –Ω–∞—Ö–æ–¥–∏—Ç–µ —Å—Ç–∞—Ä—ã–π –∫–Ω–∏–∂–Ω—ã–π —à–∫–∞—Ñ, –∑–∞ –∫–æ—Ç–æ—Ä—ã–º —Å–∫—Ä—ã–≤–∞–µ—Ç—Å—è —Ç–∞–π–Ω–∞—è –ª–µ—Å—Ç–Ω–∏—Ü–∞, –≤–µ–¥—É—â–∞—è –≥–ª—É–±–æ–∫–æ –ø–æ–¥ –∑–µ–º–ª—é.\n'
           '–ö–∞–∂–µ—Ç—Å—è, –º–∞–≥, –Ω–µ–∫–æ–≥–¥–∞ –æ–±–∏—Ç–∞–≤—à–∏–π –≤ —ç—Ç–æ–π –±–∞—à–Ω–µ —Å–∫—Ä—ã–ª—Å—è –≤ —Å—É–º—Ä–∞–∫–µ –ø–æ—Ç–∞–π–Ω–æ–≥–æ —Ö–æ–¥–∞ –∏ –Ω–µ –∑–∞–∫—Ä—ã–ª –µ–≥–æ –∑–∞ —Å–æ–±–æ–π.')

print("–ü–µ—Ä–µ–¥ –≤–∞–º–∏ —Å—Ç–æ–∏—Ç –≤—ã–±–æ—Ä:\n"
        "1. –°–ø—É—Å—Ç–∏—Ç—å—Å—è –ø–æ —Ç–µ–º–Ω–æ–π –ª–µ—Å—Ç–Ω–∏—Ü–µ.\n"
        "2. –ù–µ —Å–ø—É—Å–∫–∞—Ç—å—Å—è –∏ –ø–æ–∫–∏–Ω—É—Ç—å –±–∞—à–Ω—é.")

choice4 = input('–ß—Ç–æ –≤—ã –≤—ã–±–µ—Ä–∏—Ç–µ?: ')
if choice4 == '2':
    print_slow('–í—ã –ø–æ–∫–∏–¥–∞–µ—Ç–µ —Å—Ç–∞—Ä—É—é –±–∞—à–Ω—é.')
    print_slow('–£–ª–∏—Ü—ã –∑–∞–≤–æ–ª–æ–∫–ª–∞ –Ω–µ–ø—Ä–æ–≥–ª—è–¥–Ω–∞—è —Ç—å–º–∞. –°–ª–æ–∂–Ω–æ —Ä–∞–∑–≥–ª—è–¥–µ—Ç—å —á—Ç–æ-—Ç–æ –¥–∞–ª—å—à–µ —Å–æ–±—Å—Ç–≤–µ–Ω–Ω–æ–≥–æ –Ω–æ—Å–∞.')
    print_slow('–¢–µ–ª–æ –ª–æ–º–∏—Ç –æ—Ç —É—Å—Ç–∞–ª–æ—Å—Ç–∏. –ù—É–∂–Ω–æ –æ—Ç–¥–æ—Ö–Ω—É—Ç—å. \n'
               '–í—ã —Å–∞–¥–∏—Ç–µ—Å—å –Ω–∞ –∑–µ–º–ª—é –∏ –∑–∞–∫—Ä—ã–≤–∞–µ—Ç–µ –≥–ª–∞–∑–∞. –°–æ–Ω –æ–∫—É—Ç—ã–≤–∞–µ—Ç –≤–∞—Å.')
    print_slow('–í—ã –±–æ–ª—å—à–µ –Ω–µ –ø—Ä–æ—Å–Ω–µ—Ç–µ—Å—å. \n'
               '=== –ö–û–ù–ï–¶: –û–ë–™–Ø–¢–ò–Ø –¢–¨–ú–´ ===')                             #1 –∫–æ–Ω—Ü–æ–≤–∫–∞
    sys.exit()


print_slow('–í—ã —Å–ø—É—Å–∫–∞–µ—Ç–µ—Å—å. –í–ø–µ—Ä–µ–¥–∏ –≤—ã –≤–∏–¥–∏—Ç–µ —É–∑–∫–∏–π –∫–æ—Ä–∏–¥–æ—Ä.')
print_slow('–ë–µ—Å—Å—á–µ—Ç–Ω–æ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —á–∞—Å–æ–≤ —Ö–æ–¥—å–±—ã –ø—Ä–∏–≤–æ–¥—è—Ç –≤–∞—Å –≤ —É—â–µ–ª—å–µ.')
prompt()

print('\n=== –£—â–µ–ª—å–µ ===\n')

print_slow('–í—ã–≥–ª—è–Ω—É–≤ –∏–∑-–∑–∞ –æ—Ç–≤–µ—Å–Ω–æ–π —Å–∫–∞–ª—ã –≤—ã –≤–∏–¥–∏—Ç–µ –º–∞–≥–∞, —Å—Ç–æ—è—â–µ–≥–æ —Å–ø–∏–Ω–æ–π –∫ –≤–∞–º. –ù–∞ –Ω–µ–º —á–µ—Ä–Ω–∞—è –ø–æ—Ç—Ä–µ–ø–∞–Ω–Ω–∞—è —à–ª—è–ø–∞, –∏–∑-–ø–æ–¥ –∫–æ—Ç–æ—Ä–æ–π –≤–∏–¥–Ω–µ—é—Ç—Å—è –¥–ª–∏–Ω–Ω—ã–µ –≤–æ–ª–æ—Å—ã,\n'
           '–ø–æ–∫—Ä—ã–≤–∞—é—â–∏–µ –ø–ª–µ—á–∏, –∑–∞–∫—É—Ç–∞–Ω–Ω—ã–µ –≤ —Å—Ç–∞—Ä—ã–π –∫–æ—Ä–∏—á–Ω–µ–≤—ã–π –ø–ª–∞—â. –í—ã —É–≤–µ—Ä–µ–Ω—ã, —á—Ç–æ –Ω–µ –≤–∏–¥–µ–ª–∏ –µ–≥–æ —Ä–∞–Ω—å—à–µ.')
print_slow('–ù–∞–ø—Ä–æ—Ç–∏–≤ –º–∞–≥–∞, –Ω–∞–≤–∏—Å–∞—è –Ω–∞–¥ –Ω–∏–º, —Å–ª–æ–≤–Ω–æ –º–æ–≥—É—á–∞—è —Å–∫–∞–ª–∞, —Å–∏–¥–∏—Ç –¥—Ä–∞–∫–æ–Ω. –ï–≥–æ —á–µ—à—É—è –æ—Ç—Ä–∞–∂–∞–µ—Ç —Ç–µ–ø–ª—ã–π —Å–≤–µ—Ç —Ñ–∞–∫–µ–ª–æ–≤, —Ä–∞–∑–±—Ä–æ—Å–∞–Ω–Ω—ã—Ö –ø–æ —Å—Ç–µ–Ω–∞–º —É—â–µ–ª—å—è.')
print_slow('–û–Ω–∏ –≥–æ–≤–æ—Ä—è—Ç –æ —á–µ–º-—Ç–æ. –í—ã –Ω–µ –≤ —Å–∏–ª–∞—Ö —Ä–∞—Å—Å–ª—ã—à–∞—Ç—å –∏ —Å–ª–æ–≤–∞.')
print_slow('–°–ø—É—Å—Ç—è –Ω–µ–∫–æ—Ç–æ—Ä–æ–µ –≤—Ä–µ–º—è, –≤—ã –≤–∏–¥–∏—Ç–µ, –∫–∞–∫ –º–∞–≥ –Ω–∞—á–∏–Ω–∞–µ—Ç —É—Ö–æ–¥–∏—Ç—å, –æ—Å—Ç–∞–≤–ª—è—è –¥—Ä–∞–∫–æ–Ω–∞ —Å—Ä–µ–¥–∏ —à–∏—Ä–æ–∫–æ–≥–æ —É—â–µ–ª—å—è.')

if karma > 1:                  #–ø—É—Ç—å —Ç—å–º—ã
    print_slow('–í—ã –ø—Ä–æ–¥–æ–ª–∂–∞–µ—Ç–µ –Ω–∞–±–ª—é–¥–∞—Ç—å –∑–∞ –Ω–∏–º, –∫–∞–∫ –≤–¥—Ä—É–≥...')
    print_slow("–î—Ä–∞–∫–æ–Ω –º–µ–¥–ª–µ–Ω–Ω–æ –ø–æ–≤–æ—Ä–∞—á–∏–≤–∞–µ—Ç —Å–≤–æ—é –≥—Ä–æ–º–∞–¥–Ω—É—é –≥–æ–ª–æ–≤—É –∫ –≤–∞–º. –ï–≥–æ –≥–ª–∞–∑–∞ –≥–æ—Ä—è—Ç –∞–¥–æ–≤—ã–º –æ–≥–Ω—ë–º.")
    print_slow("‚Äî –Ø —á—É–≤—Å—Ç–≤—É—é —Ç—å–º—É –Ω–∞ —Ç–µ–±–µ, —Ä—ã—Ü–∞—Ä—å. –¢—ã –ø—Ä–∏—à—ë–ª –∫–æ –º–Ω–µ, –Ω–µ –∏—Å–∫—É–ø–∏–≤ –≤–∏–Ω—É. –¢—ã —Å—Ç—É–ø–∏–ª –Ω–∞ –ø—É—Ç—å —Ç—å–º—ã.")
    print_slow("–û —á–µ–º –æ–Ω –≥–æ–≤–æ—Ä–∏—Ç? –û –∫–∞–∫–æ–π –≤–∏–Ω–µ –∏–¥–µ—Ç —Ä–µ—á—å?.")
    print_slow("–ù–µ –ø—Ä–æ—Ö–æ–¥–∏—Ç –∏ –Ω–µ—Å–∫–æ–ª—å–∫–æ —Å–µ–∫—É–Ω–¥, –∫–∞–∫ –æ–Ω –∏–∑–≤–µ—Ä–≥–∞–µ—Ç –ø–ª–∞–º—è...")
    Character.fight(dragon, player)
    if Knight.low_hp_check(player):                            #—Ä—ã—Ü–∞—Ä—å –ø–æ—Ä–∞–∂–µ–Ω
            print_slow("–í—ã –Ω–µ –≤—ã–¥–µ—Ä–∂–∏–≤–∞–µ—Ç–µ –µ–≥–æ –∞—Ç–∞–∫–∏. –í–∞—à–µ —Ç–µ–ª–æ –ø–æ–∫—Ä—ã—Ç–æ –º–Ω–æ–≥–æ—á–∏—Å–ª–µ–Ω–Ω—ã–º–∏ –æ–∂–æ–≥–∞–º–∏. –í—ã —Å–ª–∞–±–µ–µ—Ç–µ.")
            print_slow("–°–∫–≤–æ–∑—å –ø–æ–ª—É–∑–∞–∫—Ä—ã—Ç—ã–µ –≤–µ–∫–∏ –≤—ã –≤–∏–¥–∏—Ç–µ –∑–Ω–∞–∫–æ–º—É—é —Ñ–∏–≥—É—Ä—É –≤ —á–µ—Ä–Ω–æ–º –ø–ª–∞—â–µ. –í—ã –±–æ–ª—å—à–µ –Ω–∏–∫–æ–≥–¥–∞ –µ–µ –Ω–µ —É–≤–∏–¥–µ—Ç–µ, –∫–∞–∫ –Ω–µ —É–≤–∏–¥–µ—Ç–µ –±–æ–ª—å—à–µ –Ω–∏—á–µ–≥–æ.")
            print_slow('=== –ö–û–ù–ï–¶: –ö–ê–†–ê –ó–ê –ì–†–ï–•–ò ===')                            #2 –∫–æ–Ω—Ü–æ–≤–∫–∞
            sys.exit()
    else:
        print_slow("–í—ã –≤–æ–∑–≤—ã—à–∞–µ—Ç–µ—Å—å –Ω–∞–¥ –º–µ—Ä—Ç–≤—ã–º —Ç–µ–ª–æ–º –æ–≥—Ä–æ–º–Ω–æ–≥–æ —Å—É—â–µ—Å—Ç–≤–∞. –í—ã –ø–æ–±–µ–¥–∏–ª–∏.")
        print_slow("–û–±–µ—Ä–Ω—É–≤—à–∏—Å—å, –≤—ã –≤–Ω–æ–≤—å –≤–∏–¥–∏—Ç–µ –¥–æ –±–æ–ª–∏ –∑–Ω–∞–∫–æ–º—É—é —Ñ–∏–≥—É—Ä—É –≤ –ø–ª–∞—â–µ. –ù–∞ —ç—Ç–æ—Ç —Ä–∞–∑ –æ–Ω–∞ —Å–∞–º–∞ –Ω–∞–ø—Ä–∞–≤–ª—è–µ—Ç—Å—è –∫ –≤–∞–º. –ú–µ–¥–ª–µ–Ω–Ω–æ. –ë–µ—Å—à—É–º–Ω–æ.")
        print_slow("‚Äî –¢–µ–±—è –ø–æ–≥–ª–æ—Ç–∏–ª–∞ —Ç—å–º–∞, ‚Äî –¥–æ–Ω–æ—Å—Å—è –µ–ª–µ —Å–ª—ã—à–Ω—ã–º –º–µ—Ç–∞–ª–ª–∏—á–µ—Å–∫–∏–π –≥–æ–ª–æ—Å, ‚Äî —Ç—ã —Å—Ç–∞–ª –±—Ä–µ–º–µ–Ω–µ–º –¥–ª—è —ç—Ç–æ–≥–æ –º–∏—Ä–∞.\n"
                   "–í—ã –≤—ã–∂–∏–¥–∞–µ—Ç–µ. \n"
                   "‚Äî –¢—ã –∏ –≤—Å–µ —Ä—ã—Ü–∞—Ä–∏ –ù–∏–ª—å—Ñ–≥–∞–∞—Ä–¥–∞ –ø–æ–¥–≤–µ—Ä–≥–ª–∏—Å—å —á–∞—Ä–∞–º —Ç–µ–º–Ω–æ–≥–æ –º–∞–≥–∞. –ü–æ–¥ –µ–≥–æ –≤–ª–∏—è–Ω–∏–µ–º –≤—ã —Å–æ–∂–≥–ª–∏ –≥–æ—Ä–æ–¥ –∏ —É–±–∏–ª–∏ –µ–≥–æ –∂–∏—Ç–µ–ª–µ–π. \n"
                   "–í–∞—Å –≤–¥—Ä—É–≥ –ø–æ—Ä–∞–∑–∏–ª–∏ –≤–æ—Å–ø–æ–º–∏–Ω–∞–Ω–∏—è –∂–µ—Å—Ç–æ–∫–æ–π –±–æ–π–Ω–∏. –¢–∞–∫ –ø–æ—Ä–∞–∂–∞–µ—Ç –º–æ–ª–Ω–∏—è, —Ç–∞–∫ –ø–æ—Ä–∞–∂–∞–µ—Ç —Å–∞–º—ã–π –æ—Å—Ç—Ä—ã–π –∫–ª–∏–Ω–æ–∫. –≠—Ç–æ –ø—Ä–∞–≤–¥–∞. –í—Å–µ –¥–æ –µ–¥–∏–Ω–æ–≥–æ —Å–ª–æ–≤–∞.\n"
                   "‚Äî –£ —Ç–µ–±—è –±—ã–ª —à–∞–Ω—Å –∏—Å–∫—É–ø–∏—Ç—å —Å–≤–æ—é –≤–∏–Ω—É, –Ω–æ —Ç—ã –æ—Ç–≤–µ—Ä–≥ –µ–≥–æ, ‚Äî –≥–æ–ª–æ—Å —Ñ–∏–≥—É—Ä—ã –≤ –ø–ª–∞—â–µ –ø–æ –ø—Ä–µ–∂–Ω–µ–º—É –±—ã–ª —Å–ø–æ–∫–æ–π–Ω—ã–º.\n"
                   "‚Äî –ò–¥–µ–º —Å–æ –º–Ω–æ–π, ‚Äî –≤–¥—Ä—É–≥ —Ä–µ–∑–∫–æ —Å–∫–∞–∑–∞–ª–∞ —Ñ–∏–≥—É—Ä–∞.\n"
                   "–í—ã –Ω–µ –≤ —Å–∏–ª–∞—Ö —Å–æ–ø—Ä–æ—Ç–∏–≤–ª—è—Ç—å—Å—è –∏ —Å–ª–µ–¥—É–µ—Ç–µ –∑–∞ –Ω–µ–π. –í –æ–±—ä—è—Ç–∏—è —Ç–µ–º–Ω–æ—Ç—ã —Å–∫–∞–ª–∏—Å—Ç—ã—Ö —É—â–µ–ª–∏–π.")
        print_slow('=== –ö–û–ù–ï–¶: –í–ï–°–¢–ù–ò–¶–ê –¢–¨–ú–´ ===')                                 #3 –∫–æ–Ω—Ü–æ–≤–∫–∞
        sys.exit()

else:                                   #–ø—É—Ç—å —Å–≤–µ—Ç–∞
    print("–ü–µ—Ä–µ–¥ –≤–∞–º–∏ —Å—Ç–æ–∏—Ç –≤—ã–±–æ—Ä:\n"
          "1. –£–π—Ç–∏ –∏–∑ —É—â–µ–ª—å—è –∏ –Ω–µ –Ω–∞–≤–ª–µ–∫–∞—Ç—å –Ω–∞ —Å–µ–±—è –±–µ–¥—É –≤ –≤–∏–¥–µ –¥—Ä–∞–∫–æ–Ω–∞.\n"
          "2. –í—ã–π—Ç–∏ –∫ –¥—Ä–∞–∫–æ–Ω—É –∏ –ø—Ä–∏–Ω—è—Ç—å –±–æ–π, –∫–æ—Ç–æ—Ä—ã–π –∑–∞–≥–æ—Ç–æ–≤–∏–ª–∞ —Å—É–¥—å–±–∞.")

    choice5 = input('–ß—Ç–æ –≤—ã –≤—ã–±–µ—Ä–∏—Ç–µ?: ')
    if choice5 == '2':
        print_slow('–í—ã —Ä–µ—à–∞–µ—Ç–µ –≤—ã–π—Ç–∏ –∫ –¥—Ä–∞–∫–æ–Ω—É.')
        print_slow("‚Äî –ï—Å–ª–∏ –±–æ–π —Å —Ç–æ–±–æ–π ‚Äî –º–æ—è —Å—É–¥—å–±–∞, —Ç–æ —è –≥–æ—Ç–æ–≤ –µ—ë –ø—Ä–∏–Ω—è—Ç—å!")
        print_slow("–î—Ä–∞–∫–æ–Ω —Å–º–æ—Ç—Ä–∏—Ç –Ω–∞ –≤–∞—Å –±–µ–∑ –∑–ª–æ–±—ã.")
        print_slow("‚Äî –°—É—â–µ—Å—Ç–≤–∞ —Å–∞–º–∏ –∫—É—é—Ç —Å–≤–æ—é —Å—É–¥—å–±—É, –¥–∏—Ç—è —Å–≤–µ—Ç–∞. - –æ—Ç–≤–µ—á–∞–µ—Ç –æ–Ω.")
        print_slow("‚Äî –ß—Ç–æ —Å—Ç–∞–ª–æ —Å –≥–æ—Ä–æ–¥–æ–º –Ω–µ–ø–æ–¥–∞–ª–µ–∫—É?")
        print_slow("‚Äî –ü–æ–≤–µ—Ä—å, –æ—Ç–≤–µ—Ç –Ω–∞ —ç—Ç–æ—Ç –≤–æ–ø—Ä–æ—Å –Ω–µ —Ç–∞–∫ –ø—Ä–æ—Å—Ç, –∫–∞–∫ —Ç—ã –¥—É–º–∞–µ—à—å.")
        print_slow("‚Äî –ß—Ç–æ –∂–µ –∑–¥–µ—Å—å —Å–ª–æ–∂–Ω–æ–≥–æ? –ò–ª–∏ —Ç—ã —Å–∫—Ä—ã–≤–∞–µ—à—å, —á—Ç–æ —Å–∞–º –≥–æ—Ä–æ–¥ –∏ —É–Ω–∏—á—Ç–æ–∂–∏–ª.")
        print_slow("‚Äî –ü–æ–≤–µ—Ä—å, –¥–æ–±–ª–µ—Å—Ç–Ω—ã–π —Ä—ã—Ü–∞—Ä—å, —ç—Ç–æ –Ω–µ —Ç–∞–∫.")
        print("–ü–µ—Ä–µ–¥ –≤–∞–º–∏ —Å—Ç–æ–∏—Ç –≤—ã–±–æ—Ä:\n"
              "1. –ü–æ–≤–µ—Ä–∏—Ç—å –¥—Ä–∞–∫–æ–Ω—É.\n"
              "2. –ù–µ –≤–µ—Ä–∏—Ç—å –¥—Ä–∞–∫–æ–Ω—É –∏ –Ω–∞–ø–∞—Å—Ç—å –Ω–∞ –Ω–µ–≥–æ, –æ—Ç–æ–º—Å—Ç–∏–≤ –∑–∞ –ù–∏–ª—å—Ñ–≥–∞–∞—Ä–¥.")

        choice6 = input('–ß—Ç–æ –≤—ã –≤—ã–±–µ—Ä–∏—Ç–µ?: ')
        if choice6 == '1':
            print_slow('–í—ã –≤–µ—Ä–∏—Ç–µ –¥—Ä–∞–∫–æ–Ω—É –∏ –æ—Ç—Å—Ç—É–ø–∞–µ—Ç–µ.')
            print_slow("‚Äî –ú—É–¥—Ä—ã–π –≤—ã–±–æ—Ä, ‚Äî –ø—Ä–æ–∏–∑–Ω–æ—Å–∏—Ç –î—Ä–∞–∫–æ–Ω, ‚Äî –Ω–µ –≤—Å—è–∫–∞—è –≤—Å—Ç—Ä–µ—á–∞ –¥–æ–ª–∂–Ω–∞ –æ–∫–∞–Ω—á–∏–≤–∞—Ç—å—Å—è –∫—Ä–æ–≤—å—é.")
            print_slow("–í—ã —É—Ö–æ–¥–∏—Ç–µ –∏–∑ —É—â–µ–ª—å—è, –æ—Å—Ç–∞–≤–ª—è—è –ø—Ä–æ—à–ª–æ–µ –ø–æ–∑–∞–¥–∏.")
            print_slow("–í—ã –±—É–¥–µ—Ç–µ –∂–∏—Ç—å. –ú–æ–∂–µ—Ç –±—ã—Ç—å, –¥–∞–∂–µ —Å—á–∞—Å—Ç–ª–∏–≤–æ, –≤ –Ω–µ–≤–µ–¥–µ–Ω–∏–∏.")
            print_slow("\n=== –ö–û–ù–ï–¶: –ù–ï–í–ï–î–ï–ù–ò–ï ‚Äî –ë–õ–ê–ì–û ===")
            sys.exit()                                              #4 –∫–æ–Ω—Ü–æ–≤–∫–∞
        else:
            print_slow("–í—ã —Å –∫—Ä–∏–∫–æ–º –±—Ä–æ—Å–∞–µ—Ç–µ—Å—å –≤ –∞—Ç–∞–∫—É. –î—Ä–∞–∫–æ–Ω –ª–∏—à—å –≤–∑–¥—ã—Ö–∞–µ—Ç –∏ –æ–±–æ—Ä–æ–Ω—è–µ—Ç—Å—è.")
            Character.fight(player, dragon)
            if Knight.low_hp_check(player):
                print_slow("–•–≤–æ—Å—Ç —Å–æ —Å—Ç—Ä–∞—à–Ω–æ–π —Å–∏–ª–æ–π –æ–±—Ä—É—à–∏–≤–∞–µ—Ç—Å—è –Ω–∞ –≤–∞—Å. –í—ã –ø—Ä–æ–∏–≥—Ä–∞–ª–∏ —á–µ—Å—Ç–Ω—ã–π –±–æ–π.")
                print_slow("\n=== –ö–û–ù–ï–¶: –ß–ï–°–¢–ù–´–ô –ë–û–ô ===")
                sys.exit()
            else:
                print_slow("–í—ã –Ω–∞—Ö–æ–¥–∏—Ç–µ —Å–ª–∞–±–æ–µ –º–µ—Å—Ç–æ –∏ –ø–æ–±–µ–∂–¥–∞–µ—Ç–µ. –î—Ä–∞–∫–æ–Ω –∏–∑–¥—ã—Ö–∞–µ—Ç.")
                print_slow("‚Äî –Ø –æ—Ç–æ–º—Å—Ç–∏–ª... –∑–∞ –≥–æ—Ä–æ–¥... ‚Äî –≤—ã —Ç—è–∂–µ–ª–æ –¥—ã—à–∏—Ç–µ.")
                print_slow(f"‚Äî –ë—Ä–∞–≤–æ! –ë—Ä–∞–≤–æ, –¥–æ–±–ª–µ—Å—Ç–Ω—ã–π —Ä—ã—Ü–∞—Ä—å, {hero_name}! ‚Äî —Å–æ —Å—Ç–æ—Ä–æ–Ω—ã —Ç–µ–º–Ω—ã—Ö —Å–∫–∞–ª –¥–æ–Ω–æ—Å–∏—Ç—Å—è –ª–∏–∫—É—é—â–∏–π –≥–æ–ª–æ—Å –º–∞–≥–∞.")
                print_slow("‚Äî –ï—â–µ –æ–¥–Ω–∞ –Ω–µ–≤–∏–Ω–Ω–∞—è –¥—É—à–∞, –∑–∞–≥—É–±–ª–µ–Ω–Ω–∞—è —Ç–æ–±–æ–π! ‚Äî —á–∞—Ä–æ–¥–µ–π —É–∫–∞–∑—ã–≤–∞–µ—Ç –Ω–∞ —Ç—è–∂–µ–ª–æ–µ —Ç–µ–ª–æ –¥—Ä–∞–∫–æ–Ω–∞.")
                print_slow("–í—ã –Ω–∏—á–µ–≥–æ –Ω–µ –ø–æ–Ω–∏–º–∞–µ—Ç–µ. –ß—Ç–æ –∑–¥–µ—Å—å –¥–µ–ª–∞–µ—Ç —ç—Ç–æ—Ç —á—É–¥–∏–∫?")
                print_slow('‚Äî –¢—ã —Ç–∞–∫ –Ω–∏—á–µ–≥–æ –∏ –Ω–µ –ø–æ–Ω—è–ª. –ö—Ä–æ–≤—å –Ω–∞ –¥–æ—Å–ø–µ—Ö–∞—Ö, —Å–ª–æ–º–∞–Ω—ã–π –º–µ—á...')
                print_slow("–ú–∞–≥ –ø—Ä–æ–¥–æ–ª–∂–∞–µ—Ç:\n"
                           "‚Äî –ù–∞ —Ç–≤–æ–∏—Ö —Ä—É–∫–∞—Ö –∫—Ä–æ–≤—å –∂–∏—Ç–µ–ª–µ–π –ù–∏–ª—å—Ñ–≥–∞–∞—Ä–¥–∞.\n"
                           "–í–∞—Å –≤–¥—Ä—É–≥ –ø–æ—Ä–∞–∑–∏–ª–∏ –≤–æ—Å–ø–æ–º–∏–Ω–∞–Ω–∏—è –∂–µ—Å—Ç–æ–∫–æ–π –±–æ–π–Ω–∏. –¢–∞–∫ –ø–æ—Ä–∞–∂–∞–µ—Ç –º–æ–ª–Ω–∏—è, —Ç–∞–∫ –ø–æ—Ä–∞–∂–∞–µ—Ç —Å–∞–º—ã–π –æ—Å—Ç—Ä—ã–π –∫–ª–∏–Ω–æ–∫. –≠—Ç–æ –ø—Ä–∞–≤–¥–∞. –í—Å–µ –¥–æ –µ–¥–∏–Ω–æ–≥–æ —Å–ª–æ–≤–∞.\n")
                print_slow('–í—ã –ø–∞–¥–∞–µ—Ç–µ –Ω–∞ –∫–æ–ª–µ–Ω–∏. –î–æ –∫–æ–Ω—Ü–∞ –¥–Ω–µ–π –≤–∞–º —Å—É–∂–¥–µ–Ω–æ –ø—Ä–æ–∂–∏—Ç—å —Å —ç—Ç–∏–º –∑–∞–Ω–∏–µ–º –∏ –Ω–µ–∏—Å–∫—É–ø–ª–µ–Ω–Ω—ã–º —á—É–≤—Ç—Å–≤–æ–º –≤–∏–Ω—ã.')
                print_slow("\n=== –ö–û–ù–ï–¶: –ë–†–ï–ú–Ø –í–ò–ù–´ ===")
                sys.exit()
    else:
        print_slow('–í—ã —Ä–∞–∑–≤–æ—Ä–∞—á–∏–≤–∞–µ—Ç–µ—Å—å –∏ –¥–µ–ª–∞–µ—Ç–µ —à–∞–≥, —Å–æ–±—Ä–∞–≤—à–∏—Å—å —É—Ö–æ–¥–∏—Ç—å –∏–∑ —É—â–µ–ª—å—è, \n'
                   '–∫–∞–∫ –≤–¥—Ä—É–≥ —Å–ª—ã—à–∏—Ç–µ –∑–≤–æ–Ω–∫–∏–π —Ö—Ä—É—Å—Ç —Å—É—Ö–æ–π –≤–µ—Ç–∫–∏, –æ–∫–∞–∑–∞–≤—à–µ–π—Å—è –ø–æ–¥ –≤–∞—à–∏–º–∏ –Ω–æ–≥–∞–º–∏.\n'
                   '–≠—Ö–æ —Å–∫–∞–ª —Ä–∞–∑–Ω–µ—Å–ª–æ –∑–≤—É–∫ –ø–æ –≤—Å–µ–º—É —É—â–µ–ª—å—é.')
        print_slow('–í—ã –∑–∞–º–µ—Ç–∏–ª–∏, –∫–∞–∫ –≤–∑–æ—Ä –º–∞–≥–∞ —É—Å—Ç—Ä–µ–º–∏–ª—Å—è –≤ –≤–∞—à—É —Å—Ç–æ—Ä–æ–Ω—É.')
        print_slow("–ù–∞ –ª–∏—Ü–µ –º–∞–≥–∞ –º–µ–ª—å–∫–∞–µ—Ç —É–¥–∏–≤–ª–µ–Ω–∏–µ, —Å–º–µ–Ω—è—é—â–µ–µ—Å—è —Ö–æ–ª–æ–¥–Ω–æ–π —É—Å–º–µ—à–∫–æ–π.")
        print_slow("‚Äî –ù–µ–∂–¥–∞–Ω–Ω—ã–π –≥–æ—Å—Ç—å. –õ—é–±–æ–ø—ã—Ç–Ω–æ, —Å–∫–æ–ª—å–∫–æ –æ–Ω –ø–æ–¥—Å–ª—É—à–∞–ª... –î—Ä–∞–∫–æ–Ω, –ø—Ä–∏–∫–æ–Ω—á–∏ –µ–≥–æ.")
        print_slow("–î—Ä–∞–∫–æ–Ω –≤–∑–¥—Ä–∞–≥–∏–≤–∞–µ—Ç. –í –µ–≥–æ –≥–ª–∞–∑–∞—Ö –≤—ã –≤–∏–¥–∏—Ç–µ –±–æ—Ä—å–±—É ‚Äî —Å–æ–±—Å—Ç–≤–µ–Ω–Ω—É—é –≤–æ–ª—é –∏ –º–∞–≥–∏—á–µ—Å–∫–æ–µ –ø—Ä–∏–Ω—É–∂–¥–µ–Ω–∏–µ.")
        print_slow("‚Äî –ú–Ω–µ... –∂–∞–ª—å... ‚Äî —Å–∏–ø–ª—ã–π –≥–æ–ª–æ—Å –î—Ä–∞–∫–æ–Ω–∞ –≥—Ä–µ–º–∏—Ç –≤ –≤–∞—à–µ–º —Å–æ–∑–Ω–∞–Ω–∏–∏. ‚Äî –Ø –Ω–µ –≤–ª–∞—Å—Ç–µ–Ω –Ω–∞–¥ —Å–æ–±–æ–π!")
        print_slow("–î—Ä–∞–∫–æ–Ω —Å —Ä—ë–≤–æ–º –≤—ã–¥—ã—Ö–∞–µ—Ç –ø–ª–∞–º—è. –í—ã –µ–¥–≤–∞ —É—Å–ø–µ–≤–∞–µ—Ç–µ —É–∫—Ä—ã—Ç—å—Å—è –∑–∞ —Å–∫–∞–ª–æ–π.")
        print_slow("–≠—Ç–æ –Ω–µ —á–µ—Å—Ç–Ω—ã–π –±–æ–π. –≠—Ç–æ –∫–∞–∑–Ω—å –ø–æ –ø—Ä–∏–∫–∞–∑—É.")
        Character.fight(dragon, player)
        if Knight.low_hp_check(player):                    # —Ä—ã—Ü–∞—Ä—å –ø–æ—Ä–∞–∂–µ–Ω
            print_slow("–£–≤–æ—Ä–∞—á–∏–≤–∞—Ç—å—Å—è –±–µ—Å–ø–æ–ª–µ–∑–Ω–æ. –ü–ª–∞–º—è –Ω–∞—Å—Ç–∏–≥–∞–µ—Ç –≤–∞—Å, –æ–ø–∞–ª—è—è –¥–æ—Å–ø–µ—Ö–∏ –∏ –ø–ª–æ—Ç—å.")
            print_slow("–í—ã –ø–∞–¥–∞–µ—Ç–µ, —á—É–≤—Å—Ç–≤—É—è —Å—Ç—Ä–∞—à–Ω—ã–π –∂–∞—Ä. –í–∞—à–µ —Å–æ–∑–Ω–∞–Ω–∏–µ —É–≥–∞—Å–∞–µ—Ç.")
            print_slow("–°–∫–≤–æ–∑—å –ø–µ–ª–µ–Ω—É –±–æ–ª–∏ –≤—ã –≤–∏–¥–∏—Ç–µ, –∫–∞–∫ –î—Ä–∞–∫–æ–Ω, –æ—á–Ω—É–≤—à–∏—Å—å –æ—Ç —á–∞—Ä, —Å —É–∂–∞—Å–æ–º —Å–º–æ—Ç—Ä–∏—Ç –Ω–∞ —Ç–æ, —á—Ç–æ –Ω–∞—Ç–≤–æ—Ä–∏–ª.")
            print_slow("‚Äî –ü—Ä–æ—Å—Ç–∏... ‚Äî –µ–≥–æ –≥–æ–ª–æ—Å –≤ –≤–∞—à–µ–π –≥–æ–ª–æ–≤–µ —Ç–∏—Ö–∏–π, –∫–∞–∫ —à–µ–ª–µ—Å—Ç –ª–∏—Å—Ç–≤—ã. ‚Äî –¢—ã –ø–æ–≥–∏–± –Ω–µ –Ω–∞–ø—Ä–∞—Å–Ω–æ...")
            print_slow("‚Äî –û–Ω... –ú–∞–≥... –ø–æ–≥—É–±–∏–ª –≥–æ—Ä–æ–¥... –û–±–µ—â–∞—é... –æ–Ω –æ—Ç–≤–µ—Ç–∏—Ç... –∑–∞ –Ω–∞—Å –æ–±–æ–∏—Ö...")
            print_slow("–≠—Ç–æ –∑–Ω–∞–Ω–∏–µ –ø—Ä–∏–Ω–æ—Å–∏—Ç –≤–∞–º —Å—Ç—Ä–∞–Ω–Ω–æ–µ —É—Ç–µ—à–µ–Ω–∏–µ. –í—ã –ø–æ–≥–∏–±–∞–µ—Ç–µ, –Ω–æ –≤–∞—à–∞ —Å–º–µ—Ä—Ç—å –Ω–µ –±—É–¥–µ—Ç –Ω–∞–ø—Ä–∞—Å–Ω–æ–π.")
            print_slow("\n=== –ö–û–ù–ï–¶: –ë–õ–ê–ì–û–†–û–î–ù–ê–Ø –ñ–ï–†–¢–í–ê ===")      # 5 –∫–æ–Ω—Ü–æ–≤–∫–∞
            sys.exit()
        else:
            print_slow("–î—Ä–∞–∫–æ–Ω —Å –≥—Ä–æ—Ö–æ—Ç–æ–º –ø–∞–¥–∞–µ—Ç, –∏–∑ –ø–æ—Å–ª–µ–¥–Ω–∏—Ö —Å–∏–ª –±–æ—Ä—è—Å—å —Å —á–∞—Ä–∞–º–∏.")
            print_slow("–í—ã –ø–æ–¥—Ö–æ–¥–∏—Ç–µ –∫ –ø–æ–≤–µ—Ä–∂–µ–Ω–Ω–æ–º—É –∏—Å–ø–æ–ª–∏–Ω—É.")
            print_slow("‚Äî –ù–µ... –¥–µ—Ä–∂–∏... –∑–ª–∞... ‚Äî —Ö—Ä–∏–ø–∏—Ç –æ–Ω. ‚Äî –ú–∞–≥... —ç—Ç–æ –æ–Ω... –Ω–∞—Å –æ–±–æ–∏—Ö... –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–ª...")
            print_slow("‚Äî –ì–æ—Ä–æ–¥... –æ–Ω –ø–æ–≥—É–±–∏–ª... –û–±–µ—â–∞–π... –æ—Ç–æ–º—Å—Ç–∏...")
            print_slow("–î—Ä–∞–∫–æ–Ω –∏—Å–ø—É—Å–∫–∞–µ—Ç –ø–æ—Å–ª–µ–¥–Ω–∏–π –≤–∑–¥–æ—Ö. –¢–µ–ø–µ—Ä—å –≤–∞—à –ø—É—Ç—å —è—Å–µ–Ω.")
            print_slow("–¢—ã –ø–æ–≤–æ—Ä–∞—á–∏–≤–∞–µ—à—å—Å—è –∫ –º–∞–≥—É. –¢–æ—Ç —Å—Ç–æ–∏—Ç, —Å–∫—Ä–µ—Å—Ç–∏–≤ —Ä—É–∫–∏, —Å –ª–∏—Ü–æ–º, –ø–æ–ª–Ω—ã–º –ø—Ä–µ–∑—Ä–µ–Ω–∏—è.")
            print_slow("‚Äî –ñ–∞–ª–∫–∏–π —á–µ–ª–æ–≤–µ–∫. –¢—ã –ª–∏—à—å –ø–µ—à–∫–∞ –≤ –º–æ–µ–π –∏–≥—Ä–µ. –ù–æ —Ä–∞–∑ —É–∂ —Ñ–∏–≥—É—Ä—ã –≤—ã—à–ª–∏ –∏–∑-–ø–æ–¥ –∫–æ–Ω—Ç—Ä–æ–ª—è...")
            Character.fight(player, magician)
            if Knight.low_hp_check(player):
                print_slow("–¢—ã –ø–∞–¥–∞–µ—à—å, —á—É–≤—Å—Ç–≤—É—è, –∫–∞–∫ –∂–∏–∑–Ω—å —É—Ç–µ–∫–∞–µ—Ç –≤ —Ö–æ–ª–æ–¥–Ω—ã–π –∫–∞–º–µ–Ω—å.")
                print_slow("‚Äî –ñ–∞–ª–∫–∏–π. –Ø —Å–æ–∑–¥–∞–≤–∞–ª –∏–º–ø–µ—Ä–∏–∏, –∞ —Ç—ã –¥—É–º–∞–ª, —á—Ç–æ –æ–¥–∏–Ω —Ä—ã—Ü–∞—Ä—å —á—Ç–æ-—Ç–æ –∏–∑–º–µ–Ω–∏—Ç? ‚Äî —ç—Ç–æ –ø–æ—Å–ª–µ–¥–Ω–µ–µ, —á—Ç–æ —Ç—ã —Å–ª—ã—à–∏—à—å.")
                print_slow("\n=== –ö–û–ù–ï–¶: –ü–ï–®–ö–ê –í –ß–£–ñ–û–ô –ò–ì–†–ï ===")                   #6 –∫–æ–Ω—Ü–æ–≤–∫–∞
                sys.exit()
            else:
                print_slow("‚Äî –ù-–Ω–µ–≤–æ–∑–º–æ–∂–Ω–æ... ‚Äî –º–∞–≥ –ø–∞–¥–∞–µ—Ç –Ω–∞ –∫–æ–ª–µ–Ω–∏, –∞ –∑–∞—Ç–µ–º –±–µ–∑–∂–∏–∑–Ω–µ–Ω–Ω–æ —Ä–∞—Å–ø–ª–∞—Å—Ç—ã–≤–∞–µ—Ç—Å—è –Ω–∞ –∑–µ–º–ª–µ.")
                print_slow("–¢—ã —Å—Ç–æ–∏—à—å –Ω–∞–¥ —Ç–µ–ª–æ–º –ø–æ–≤–µ—Ä–∂–µ–Ω–Ω–æ–≥–æ –≤—Ä–∞–≥–∞. –ì–æ—Ä–æ–¥ –æ—Ç–æ–º—â–µ–Ω. –î—Ä–∞–∫–æ–Ω –∏ –∂–µ—Ä—Ç–≤—ã –º–∞–≥–∞ —Å–º–æ–≥—É—Ç —É–ø–æ–∫–æ–∏—Ç—å—Å—è.")
                print_slow("–¢—ã –∏—Å–ø–æ–ª–Ω–∏–ª —Å–≤–æ–π —Ä—ã—Ü–∞—Ä—Å–∫–∏–π –¥–æ–ª–≥. –í–æ–∑–º–æ–∂–Ω–æ, —Ç–µ–ø–µ—Ä—å –ø–∞–º—è—Ç—å –≤–µ—Ä–Ω—ë—Ç—Å—è, –∏ —Ä–∞–Ω—ã –Ω–∞—á–Ω—É—Ç –∑–∞–∂–∏–≤–∞—Ç—å.")
                print_slow("\n=== –ö–û–ù–ï–¶: –ò–°–ü–û–õ–ù–ï–ù–ù–´–ô –î–û–õ–ì ===")                           #7 –∫–æ–Ω—Ü–æ–≤–∫–∞
                sys.exit()
